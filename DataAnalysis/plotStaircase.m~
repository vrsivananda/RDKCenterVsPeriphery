function plotStaircase(centerArray, peripheryArray)

trials = 1:100;

linespec = '- .';
width = 2;
size = 10;
minY = 1;
maxY = 100;


plot(trials, centerArray, linespec, 'LineWidth', width, 'MarkerSize', size);
hold on;
plot(trials, peripheryArray, linespec, 'LineWidth', width, 'MarkerSize', size);

